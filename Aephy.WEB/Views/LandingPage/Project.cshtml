                         '<div>' +
                         '<p>' + value.Feedback_Message + '</p>' +
                         '</div>' +
                         '</div>' +
                         '</div>' +
                         '</div></div>';
                 });
                 $('#freelancer-feedback-Details').html(reviewData)
                 $("#preloader").hide();
             },
             error: function (result) {
               $("#preloader").hide();
               showToaster("error", "Error !", result.responseText);
             }
          });
    }

    function BindFreelancerReview(freelancerData) {
        var totalLength = freelancerData.length;
        $(".freelancerlblTotalReviews").html(totalLength);

        var totalcommunicationRating = 0;
        var totalcollaborationRating = 0;
        var totalprofessionalismRating = 0;
        var totaltechnicalRating = 0;
        var totalsatisfactionRating = 0;
        var totalresponsivenessRating = 0;
        var totalFeedbackValues = 0;
        var totalFeedback = 0.0;
        var fivestar = 0;
        var fourstar = 0;
        var threestar = 0;
        var twostar = 0;
        var onestar = 0;

        if (totalLength != 0) {
            $.each(freelancerData, function (data, value) {
                totalcommunicationRating += value.CommunicationRating;
                totalcollaborationRating += value.CollaborationRating;
                totalprofessionalismRating += value.ProfessionalismRating;
                totaltechnicalRating += value.TechnicalRating;
                totalsatisfactionRating += value.SatisfactionRating;
                totalresponsivenessRating += value.ResponsivenessRating

                totalFeedbackValues += value.CommunicationRating + value.CollaborationRating + value.ProfessionalismRating + value.TechnicalRating + value.SatisfactionRating + value.ResponsivenessRating;

                totalFeedback = Math.floor((value.CommunicationRating + value.CollaborationRating + value.ProfessionalismRating + value.TechnicalRating + value.SatisfactionRating + value.ResponsivenessRating) / 6);

                if (totalFeedback == 1 || totalFeedback == 2) {
                    onestar += 1;
                }
                if (totalFeedback == 3 || totalFeedback == 4) {
                    twostar += 1;
                }
                if (totalFeedback == 5 || totalFeedback == 6) {
                    threestar += 1;
                }
                if (totalFeedback == 7 || totalFeedback == 8) {
                    fourstar += 1;
                }
                if (totalFeedback == 9 || totalFeedback == 10) {
                    fivestar += 1;
                }
            });

            totalcommunicationRating = totalcommunicationRating / totalLength;
            totalcollaborationRating = totalcollaborationRating / totalLength;
            totalprofessionalismRating = totalprofessionalismRating / totalLength;
            totaltechnicalRating = totaltechnicalRating / totalLength;
            totalsatisfactionRating = totalsatisfactionRating / totalLength;
            totalresponsivenessRating = totalresponsivenessRating / totalLength;

            // Binding Reviews for all
            var finalReviews = totalFeedbackValues / totalLength / 10;
            if (totalLength != 0) {
                $(".freelancerlblTotalRatings").html(finalReviews.toFixed(1))
            } 

            // Binding Reviews for all
            $("#communicationrating").html(totalcommunicationRating.toFixed(1))
            $("#collaborationrating").html(totalcollaborationRating.toFixed(1));
            $("#professionalismrating").html(totalprofessionalismRating.toFixed(1));
            $("#technicalrating").html(totaltechnicalRating.toFixed(1));
            $("#satisfactionrating").html(totalsatisfactionRating.toFixed(1));
            $("#responsivenessrating").html(totalresponsivenessRating.toFixed(1));
            // Binding Reviews for all


              // Binding stars for solutions
                $("#clientfivestar").html(fivestar)
                $("#clientfourstar").html(fourstar)
                $("#clientthreestar").html(threestar)
                $("#clienttwostar").html(twostar)
                $("#clientonestar").html(onestar)
                // Binding stars for solutions

                // Binding progress for solutions
                var onestarProgress = (onestar / totalLength) * 100;
                $('#clientonestarprogress').attr('value',onestarProgress);

                var twostarProgress = (twostar / totalLength) * 100;
                $('#clienttwostarprogress').attr('value',twostarProgress);

                var threeProgress = (threestar / totalLength) * 100;
                $('#clientthreestarprogress').attr('value',threeProgress);

                var fourProgress = (fourstar / totalLength) * 100;
                $('#clientfourstarprogress').attr('value',fourProgress);

                var fivestarProgress = (fivestar / totalLength) * 100;
                $('#clientfivestarprogress').attr('value',fivestarProgress);
            // Binding progress for solutions
        }else{
            ResetFreelancerReview();

        }
    }

    function ResetFreelancerReview(){
         $("#communicationrating").html(0.0);
           $("#collaborationrating").html(0.0);
           $("#professionalismrating").html(0.0);
           $("#technicalrating").html(0.0);
           $("#satisfactionrating").html(0.0);
           $("#responsivenessrating").html(0.0);
           $(".freelancerlblTotalRatings").html("0.0")
           $("#clientfivestar").html(0)
           $("#clientfourstar").html(0)
           $("#clientthreestar").html(0)
           $("#clienttwostar").html(0)
           $("#clientonestar").html(0)
           $('#clientonestarprogress').attr('value',0);
           $('#clienttwostarprogress').attr('value',0);
           $('#clientthreestarprogress').attr('value',0);
           $('#clientfourstarprogress').attr('value',0);
           $('#clientfivestarprogress').attr('value',0);
    }
    
