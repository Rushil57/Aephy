             }

              //var mileStoneCheckout = false;
              //  if(checkCheckoutType){
              //      mileStoneCheckout = checkCheckoutType;
              //  }
            var Industryid = @ViewData["Industry"]; 
            var Solutionid = @ViewData["Solution"];

            var data = {
