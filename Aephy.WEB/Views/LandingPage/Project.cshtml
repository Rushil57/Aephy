    function BindProjectPricingBasedOnFreelancerHourlyRate(hourlyrateData,TotalMilestoneDays,CurrentCurrency){

       // var TotalMilestoneDays = $("#MilestoneDays").val();
        var HoursPerDay = 8;
        var countPricing = 0;
        var IsSmallPriceAvailable = false;
        var IsMeduimPriceAvailable = false;
        var IsLargePriceAvailable = false;

         $.each(TotalMilestoneDays, function (milestonedata, milestonevalue) {
                // small project price counting
                if(milestonevalue.ProjectType == "SMALL"){
                    IsSmallPriceAvailable = true;
                    if(milestonevalue.Days > 0){
                          var TotalMilestoneDays = milestonevalue.Days;
                         var smallProjectList = hourlyrateData.slice(0,2);
                         $.each(smallProjectList, function (data, value) {
                             var HourlyRate = parseInt(value.HourlyRate);
                             var freelancerPriceCount = (HourlyRate * HoursPerDay) *  TotalMilestoneDays;
                             countPricing += freelancerPriceCount
                         });  
                         var finalPrice = countPricing.toLocaleString('en-US');
                         $(".cls-smallProjectprice").html(CurrentCurrency + finalPrice);
                    }
                    else{
                        $(".cls-smallProjectprice").html(CurrentCurrency + countPricing);
                    }  
                }
                // small project price counting

               // medium project price counting
               countPricing = 0;
               if(milestonevalue.ProjectType == "MEDIUM"){
                   IsMeduimPriceAvailable = true
                    if(milestonevalue.Days > 0){
                      var MediumTotalMilestoneDays = milestonevalue.Days
                      var mediumProjectList = hourlyrateData.slice(0,3);
                         $.each(mediumProjectList, function (data, value) {
                             var HourlyRate = parseInt(value.HourlyRate);
                             var freelancerPriceCount = (HourlyRate * HoursPerDay) *  MediumTotalMilestoneDays;
                             countPricing += freelancerPriceCount
                         }); 
                           var finalPrice = countPricing.toLocaleString('en-US');
                         $(".cls-mediumProjectprice").html(CurrentCurrency + finalPrice);
                    }
                    else{
                         $(".cls-mediumProjectprice").html(CurrentCurrency + countPricing);
                    }
               }
               // medium project price counting


               // large project price counting
                countPricing = 0;
                if(milestonevalue.ProjectType == "LARGE"){
                    IsLargePriceAvailable = true
                    if(milestonevalue.Days > 0){
                         var LargeTotalMilestoneDays = milestonevalue.Days
                         var largeProjectList = hourlyrateData.slice(0,5);
                         $.each(largeProjectList, function (data, value) {
                             var HourlyRate = parseInt(value.HourlyRate);
                             var freelancerPriceCount = (HourlyRate * HoursPerDay) *  LargeTotalMilestoneDays;
                             countPricing += freelancerPriceCount
                         });
                          var finalPrice = countPricing.toLocaleString('en-US');
                           $(".cls-largeProjectprice").html(CurrentCurrency + finalPrice);
                    }
                    else{
                      $(".cls-largeProjectprice").html(CurrentCurrency + countPricing);
                    }
                }
                // large project price counting
         })

         if(IsSmallPriceAvailable == false){
             $(".cls-smallProjectprice").html(CurrentCurrency + "0");
         }
         if(IsMeduimPriceAvailable == false){
             $(".cls-mediumProjectprice").html(CurrentCurrency + "0");
         }
         if(IsLargePriceAvailable == false){
              $(".cls-largeProjectprice").html(CurrentCurrency + "0");
         }

    }

    function BindSolutionFeedback(feedbackData) {
        var totalfeedbackLength = feedbackData.length;

        var totalwelldefinedprojectscope = 0;
        var totaladherenceprojectscope = 0;
        var totaldeliverablesquality = 0;
        var totalmeetingtimeliness = 0;
        var totalclientsatisfaction = 0;
        var totaladherencebudget = 0;
        var totalFeedback = 0.0;
        var fivestar = 0;
        var fourstar = 0;
        var threestar = 0;
        var twostar = 0;
        var onestar = 0;
        var feedbackShow = 1;
        var totalFeedbackValues = 0;

        $(".lblTotalReviews").html(totalfeedbackLength)
        if (totalfeedbackLength != 0) {
            $("#reviewDetails").show()
            $("#reviewn-btn").show();
            var reviewData = "";
            $.each(feedbackData, function (data, value) {
                totalwelldefinedprojectscope += value.WellDefinedProjectScope;
                totaladherenceprojectscope += value.AdherenceToProjectScope;
                totaldeliverablesquality += value.DeliverablesQuality;
                totalmeetingtimeliness += value.MeetingTimeliness;
                totalclientsatisfaction += value.Clientsatisfaction;
                totaladherencebudget += value.AdherenceToBudget

                totalFeedbackValues += value.WellDefinedProjectScope + value.AdherenceToProjectScope + value.DeliverablesQuality + value.MeetingTimeliness + value.Clientsatisfaction + value.AdherenceToBudget;

                totalFeedback = Math.floor((value.WellDefinedProjectScope + value.AdherenceToProjectScope + value.DeliverablesQuality + value.MeetingTimeliness + value.Clientsatisfaction + value.AdherenceToBudget) / 6);
                if(totalFeedback == 1 || totalFeedback == 2){
                    onestar += 1;
                }
                if(totalFeedback == 3 || totalFeedback == 4){
                    twostar += 1;
                }
                if(totalFeedback == 5 || totalFeedback == 6){
                    threestar += 1;
                }
                if(totalFeedback == 7 || totalFeedback == 8){
                    fourstar += 1;
                }
                if(totalFeedback == 9 || totalFeedback == 10){
                    fivestar += 1;
                }

                if(feedbackShow <= 2){
                    reviewData += '<div class="card info-card cls-mainreviewdiv-card">' +
                    '<div class="cls-review-card">' +
                    '<div class="d-flex align-items-center">' +
                    '<div class="ps-3">' +
                    '<h6 class="cls-feedback-clientname">'+value.ClientId +'</h6>' +
                    '<div>' +
                    '<p>'+value.Feedback_Message+'</p>' +
                    '</div>' +
                    '</div>' +
                    '</div>' +
                    '</div></div>';
                }
                

                feedbackShow++;
            })
             $('#feedbackDiv').html(reviewData)
            totalwelldefinedprojectscope = totalwelldefinedprojectscope / totalfeedbackLength;
            totaladherenceprojectscope = totaladherenceprojectscope / totalfeedbackLength;
            totaldeliverablesquality = totaldeliverablesquality / totalfeedbackLength;
            totalmeetingtimeliness = totalmeetingtimeliness / totalfeedbackLength;
            totalclientsatisfaction = totalclientsatisfaction / totalfeedbackLength;
            totaladherencebudget = totaladherencebudget / totalfeedbackLength;
            // $('#fullFeedbackShowButton').removeClass('HidefullFeedbackShowButton'); 
        }else{
            $("#reviewn-btn").hide();
        }
        $("#reviewDetails").hide()
        //$('#fullFeedbackShowButton').addClass('HidefullFeedbackShowButton'); 
      
        // Binding Reviews for all
        var finalReviews = totalFeedbackValues / (6 * totalfeedbackLength)
        if(totalfeedbackLength != 0){
            $(".lblTotalRatings").html(finalReviews.toFixed(1))
        }else{
            $(".lblTotalRatings").html("0.0")
        }
        
        $("#welldefined").html(totalwelldefinedprojectscope.toFixed(1))
        $("#adherencescope").html(totaladherenceprojectscope.toFixed(1));
        $("#qualitydeliverables").html(totaldeliverablesquality.toFixed(1));
        $("#mettingtimeliness").html(totalmeetingtimeliness.toFixed(1));
        $("#clientsatisfaction").html(totalclientsatisfaction.toFixed(1));
        $("#adherencebudget").html(totaladherencebudget.toFixed(1));
        // Binding Reviews for all

        // Binding stars for solutions
        $("#fivestar").html(fivestar)
        $("#fourstar").html(fourstar)
        $("#threestar").html(threestar)
        $("#twostar").html(twostar)
        $("#onestar").html(onestar)
        // Binding stars for solutions

        // Binding progress for solutions
            var onestarProgress = (onestar / totalfeedbackLength) * 100;
            $('#onestarprogress').attr('value',onestarProgress);

            var twostarProgress = (twostar / totalfeedbackLength) * 100;
            $('#twostarprogress').attr('value',twostarProgress);

            var threeProgress = (threestar / totalfeedbackLength) * 100;
            $('#threestarprogress').attr('value',threeProgress);

            var fourProgress = (fourstar / totalfeedbackLength) * 100;
            $('#fourstarprogress').attr('value',fourProgress);

            var fivestarProgress = (fivestar / totalfeedbackLength) * 100;
            $('#fivestarprogress').attr('value',fivestarProgress);
        // Binding progress for solutions
    }

