                                                            <td><label id="fivestar"></label></td>
                                                        </tr>
                                                        <tr>
                                                            <td>7 To 8 Stars</td>
                                                            <td><progress id="fourstarprogress" value="7" max="100"></progress></td>
                                                            <td><label id="fourstar"></label></td>
                                                        </tr>
                                                        <tr>
                                                            <td>5 To 6 Stars</td>
                                                            <td><progress id="threestarprogress" value="1" max="100"></progress></td>
                                                            <td><label id="threestar"></label></td>
                                                        </tr>
                                                        <tr> 
                                                            <td>3 To 4 Stars</td>
                                                            <td><progress id="progress" value="0" max="100"></progress></td>
                                                            <td><label id="twostar"></label></td>
