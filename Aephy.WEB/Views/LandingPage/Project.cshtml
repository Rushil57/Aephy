                                        <div class="row">
                                            <div class="col-lg-6 col-12">
                                                <table id="tableRatingBreakdown">
                                                    <tbody>
                                                        <tr>
                                                            <td>Well-Defined Project Scope</td>
                                                            <td class="rating"><i class="bi bi-star-fill"></i><label id="welldefined"></label></td>
                                                        </tr>
                                                        <tr>
                                                            <td>Adherence to Project Scope</td>
                                                            <td class="rating"><i class="bi bi-star-fill"></i><label id="adherencescope"></label></td>
                                                        </tr>
                                                        <tr>
                                                            <td>Quality of Deliverables</td>
                                                            <td class="rating"><i class="bi bi-star-fill"></i><label id="qualitydeliverables"></label></td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>

                                            <div class="col-lg-6 col-12">
                                                <table id="tableRatingBreakdown">
                                                    <tbody>
                                                        <tr>
                                                            <td>Meeting Timeliness</td>
                                                            <td class="rating"><i class="bi bi-star-fill"></i><label id="mettingtimeliness"></label></td>
                                                        </tr>
                                                        <tr>
                                                            <td>Client Satisfaction</td>
                                                            <td class="rating"><i class="bi bi-star-fill"></i><label id="clientsatisfaction"></label></td>
                                                        </tr>
                                                        <tr>
                                                            <td>Adherence to Budget</td>
                                                            <td class="rating"><i class="bi bi-star-fill"></i><label id="adherencebudget"></label></td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div>
                                <div class="pt-3" id="reviewDetails">
                                    <h5 class="justify-content-start align-items-center label-title-size title-path"><label style="background-color:#fff; padding:0 0 0 0.5em;">Reviews Details</label></h5>
                                </div>
                                <div id="feedbackDiv">
                                

                                </div>
                                <div id="reviewn-btn">
                                    <div class="view_action_Link d-flex" id="fullFeedbackShowButton">
                                                <span style="background-color:#d38b4e; width:1rem;"></span>
                                                <div class="d-flex justify-content-center align-items-center">
                                                <a onclick="OpenFeedbackProjectModalPop()">View All Feedbacks</a>
                                            <img src="/assets/img/dashboard/icons-send.png">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--Row 3 Personalize Project-->
                        <!-- Reviews row -->
                        <div class="row justify-content-center reviews px-0 gx-0" id="reviews">
                            <h3 class="pt-2 justify-content-center label-title-size title-path">
                                Top Professionals
                            </h3>
                            <div id="projectReviewCarousel" class="carousel slide" data-interval="false">
                                <div class="carousel-inner" role="listbox" id="top-professional-div">
                                
                                </div>
                                <a class="carousel-control-prev bg-transparent w-aut top-indicators" href="#projectReviewCarousel" role="button" data-bs-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                </a>
                                <a class="carousel-control-next bg-transparent w-aut top-indicators" href="#projectReviewCarousel" role="button" data-bs-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                </a>
                            </div>
                        </div>            
                    </div>
                </section>
            </div>

            <!--Second Main Column-->
            <div class="col-lg-5 col-12 rightStickyRow">
                <!-- First Row -->
                <div class="row mx-lg-4" id="solutioninfo-popUp">
                    <!-- first column -->
                    <div class="col-lg-12 rightSticky-box justify-content-center align-items-center">
                        <div class="row m-lg-2 m-4 d-flex justify-content-center align-center RButtons">
                            <div class="col-lg-6 col-md-6 d-flex justify-content-center">
                                <input type="radio" name="preferOption" label="Select Project" value="select" checked  id="ProjectSelectionButton"/>
                            </div>
                            <div class="col-lg-6 col-md-6 d-flex justify-content-center">
                                <input type="radio" name="preferOption" value="customize" label="Select Freelancer" onclick="OpenCustomisePopUp()" id="customizedradioButton"/>
                                <input type="radio" name="preferOption" value="freelancer" label="Select Freelancers" onclick="OpenFreelancerPopUp()" style="display:none" id="freelancerRadioButton"/>
                            </div>
                        </div>
                        <div class="row m-lg-2 pt-4">
                            <h5 class="section-detail-size">
                                Select size and approximate price 
                                &nbsp;<a data-toggle="popover" class="info-popover" data-bs-content="Prices shown are an estimated average and may vary based on the freelancers' hourly rates and availability."><i class="bi bi-info-circle-fill cls-team-info-icon"></i></a>
                            </h5>
                        </div>
                        <div class="row m-lg-2 pt-2 pb-2 d-flex justify-content-center align-center border-bottom border-black" style="text-align:center;">
                            <div class="col-3">
                                <label class="radio-inline" style="text-align:center;">
                                    <input class="form-check-input" type="radio" name="projSizeweb" value="small" checked onchange="GetProjectTypeValue(this)"/>
                                    <h4>Small</h4>
                                    <h5 style="opacity:0.5;" id="small-projectPrice" class="cls-smallProjectprice"></h5>
                                </label>
                            </div>
                            <div class="col-3">
                                <label class="radio-inline" style="text-align:center;">
                                    <input class="form-check-input" type="radio" name="projSizeweb" value="medium" onchange="GetProjectTypeValue(this)"/>
                                    <h4>Medium</h4>
                                    <h5 style="opacity:0.5;" id="medium-projectPrice" class="cls-mediumProjectprice"></h5>
                                </label>
                            </div>
                            <div class="col-3">
                                <label class="radio-inline" style="text-align:center;">
                                    <input class="form-check-input" type="radio" name="projSizeweb" value="large" onchange="GetProjectTypeValue(this)"/>
                                    <h4>Large</h4>
                                    <h5 style="opacity:0.5;" id="large-projectPrice" class="cls-largeProjectprice"></h5>
                                </label>
                            </div>
                            <div class="col-3">
                               <label class="radio-inline" style="text-align:center;">
                                  <input class="form-check-input cls-custom" type="radio" name="projSizeweb" value="custom" onchange="GetProjectTypeValue(this)"/>
                                  <h4 class="cls-customTitle">Custom</h4>
                                  <h5 style="opacity:0.5;" class="cls-customProjectprice">-</h5>
                               </label>
                            </div>
                        </div>
                        <div class="projectDetails-div">
                              <div class="row me-lg-2 me-4 detailRow">
                                 <div class="col key section-detail-size">Duration</div>
                                 <div class="col val text-end section-detail-size cls-duration"></div>
                              </div>
                              <div class="row me-lg-2 me-4 detailRow">
                                <div class="col key section-detail-size">Team Size &nbsp;<a data-toggle="popover" class="team-popover"><i class="bi bi-info-circle-fill cls-team-info-icon"></i></a></div>
                                <div class="col val text-end section-detail-size cls-team-size"></div>
                              </div>
                        </div>
                        <div class="projectPoints-div">

                        </div>
                        <div class="row d-flex justify-content-center align-center">
                            <div class="cls-initiate-div">
                            <button type="button" class="btn btn-brand btn-contract" onclick="OpenClientWorkingHoursPopUp()" style="width: 50%;">Initiate Contract</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <input type="hidden" value="0" id="MilestoneDays" />
        </div>
    </div>
</main>

<!-- Milestones Modal -->
