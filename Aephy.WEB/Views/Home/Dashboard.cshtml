    function sendChatMessage(){
        let msg = $("#txt_message").val();

        if(msg == "")
        {
            return false;
        }

        let senderName = "@Context.Session.GetString("FullName").ToString()";

        let model = {
            sedername : senderName,
            recivername : username,
            message : msg,
            timestamp : Date.now(),
            image : ''
        }

        $("#txt_message").val("");
        sendMessage(model);
        model = null;
    }

