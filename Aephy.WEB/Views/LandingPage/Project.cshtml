          var Solutionid = @ViewData["Solution"];

          var data = {
            IndustryId : Industryid,
            SolutionId : Solutionid
          }
          $("#preloader").show();
          $.ajax({
             type: "POST",
             contentType: "application/json; charset=utf-8",
             dataType: "json",
             url: "@Url.Action("GetAllFeedbacks", "LandingPage")",
             data: JSON.stringify(data),
             success: function (result) {
                 $("#FeedbackModal").modal('show');
                 var reviewData = "";
                 $.each(result.Result, function (data, value) {
                     reviewData += '<div class="card info-card cls-mainreviewdiv-card">' +
                         '<div class="cls-review-card">' +
                         '<div class="d-flex align-items-center">' +
                         '<div class="ps-3">' +
                         '<h6 class="cls-feedback-clientname">' + value.ClientId + '</h6>' +
                         '<div class="stars">';
                     var ratingCeilCount = Math.ceil(value.Rate);
                      for (var iterator = 1; iterator <= 10; iterator++) {
                          if (iterator <= ratingCeilCount) {
                              reviewData += '<i class="bi bi-star-fill checked_star"></i>';
                          } else {
                              reviewData += '<i class="bi bi-star-fill unchecked_star"></i>';
                          }
                      }
                     reviewData +='</div>' +
                     '<div class="row">' +
                     '<label class="col-4">' +value.Rate+'/10</label>' + 
                     '<label class="col" style="color:gray;">' + moment(value.CreateDateTime).format('DD-MM-YYYY') + '</label>' + 
                     '</div>' +
                         '<div style="padding-top: 10%;">' +
                         '<p>' + value.Feedback_Message + '</p>' +
                         '</div>' +
                         '</div>' +
                         '</div>' +
                         '</div></div>';
                 });
                 $('#feedback-list-Details').html(reviewData)
                 $("#preloader").hide();
             },
             error: function (result) {
               $("#preloader").hide();
               showToaster("error", "Error !", "Something went wrong !!");
             }
          });
    }

